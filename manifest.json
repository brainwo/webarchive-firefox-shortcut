{
  "manifest_version": 3,
  "name": "Wayback Machine",
  "short_name": "wayback_machine_web_archive",
  "description": "Shortcut to open the latest cached page in Internet Archive Wayback Machine",
  "version": "1.0.1",
  "icons": {
    "48": "icons/archive.jpg"
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "Wayback Machine",
      "keyword": "webarchive",
      "favicon_url": "icons/archive.ico",
      "search_url": "https://web.archive.org/{searchTerms}"
    }
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "WaybackMachine@web.archive.org",
      "strict_min_version": "109.0"
    }
  },
  "author": "Brian Wo",
  "homepage_url": "https://github.com/brainwo/webarchive-firefox-shortcut",
  "license": "unlicense"
}
